<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CPF_min_max</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    NOT(ISBLANK(CPF__c)),
     (LEN(CPF__c) &lt; 11) || (LEN(CPF__c ) &gt; 14))</errorConditionFormula>
    <errorDisplayField>CPF__c</errorDisplayField>
    <errorMessage>O CPF deve conter no mínimo 11 e no máximo 14.</errorMessage>
</ValidationRule>
