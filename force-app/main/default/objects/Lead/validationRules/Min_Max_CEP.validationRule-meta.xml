<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Min_Max_CEP</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    NOT(ISBLANK(PostalCode)),
     (LEN(PostalCode) &lt; 8) || (LEN(PostalCode) &gt; 10)
)</errorConditionFormula>
    <errorDisplayField>PostalCode</errorDisplayField>
    <errorMessage>O CEP deve ser preenchido com o mínimo de 8 e máximo 9.</errorMessage>
</ValidationRule>
